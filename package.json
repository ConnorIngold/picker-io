{
  "name": "pickerio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && cpx \"src/db/migrations/**/*\" dist/db/migrations && cpx \"src/client/dist/**/*.*\" dist/db/public && prisma generate --schema=./src/db/schema.prisma",
    "build:watch": "tsc --watch",
    "build:client": "cd client && npm run build",
    "compile": "tsc && node app.js",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon dist/app.js\" \"cpx -w \"src/db/migrations/**/*\" dist/db/migrations",
    "start": "node dist/app.js",
    "migrate": "prisma migrate dev --name init --schema=./src/db/schema.prisma",
    "generate": "prisma generate --schema=./src/db/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "cpx2": "^4.2.3",
    "morgan": "^1.10.0",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@shopify/shopify-api": "^6.2.0",
    "@shopify/shopify-app-express": "^1.2.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "express": "^4.18.2",
    "helmet": "^6.1.5",
    "nodemon": "^2.0.20",
    "shopify-api-node": "^3.12.3"
  }
}